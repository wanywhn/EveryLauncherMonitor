<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.gitee.wanywhn.EveryLauncherMonitor">
    <signal name="fileWrited">
      <arg name="files" type="as" direction="out"/>
    </signal>
    <method name="myrun">
    </method>
    <method name="setWatchPaths">
      <arg name="paths" type="as" direction="in"/>
    </method>
    <method name="setFileMonitorInter">
      <arg name="sec" type="i" direction="in"/>
    </method>
  </interface>
</node>
